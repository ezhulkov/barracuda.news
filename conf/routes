
GET           /                         controllers.GuiController.index
GET           /topic/:name              controllers.GuiController.topic(name:String)
GET           /article                  controllers.GuiController.article
GET           /about                    controllers.GuiController.about
GET           /adv                      controllers.GuiController.adv
GET           /contacts                 controllers.GuiController.contacts
POST          /search                   controllers.GuiController.search(q:String)

GET           /admin                    controllers.AdminController.index
GET           /admin/layout             controllers.AdminController.layout
GET           /admin/article/:id        controllers.AdminController.article(id:Long)
DELETE        /admin/article/:id        controllers.AdminController.articleDelete(id:Long)
POST          /admin/article            controllers.AdminController.articlePost

GET           /assets/*file             controllers.Assets.versioned(path="/public", file: Asset)
